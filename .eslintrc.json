{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prefer-const": "off",
    "no-unused-vars": "warn",
    "no-use-before-define": "warn",
    "no-param-reassign": "warn",
    "no-empty": "off"
  }
}
